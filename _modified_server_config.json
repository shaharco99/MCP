{
  "mcpServers": {
    "local-server": {
      "type": "stdio",
      "url": "http://localhost:8000/mcp",
      "description": "Local DevOps MCP server with tools",
      "singleResponsePerToolCall": true,
      "toolTimeout": 120
    },
    "kubernetes": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "kubernetes-mcp-server@latest"
      ],
      "env": {
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "git": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "git-mcp-server@latest"
      ],
      "env": {
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "terraform": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "terraform-mcp-server@latest"
      ],
      "env": {
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "version": "0.0.1-seed",
      "env": {
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "C:/Users/shahar/Repos"
      ],
      "description": "Filesystem MCP server for read/write access to local repository",
      "env": {
        "PYTHONWARNINGS": "ignore",
        "LOG_LEVEL": "WARNING",
        "CHUK_LOG_LEVEL": "WARNING",
        "MCP_LOG_LEVEL": "WARNING"
      }
    }
  },
  "providers": {
    "ollama": {
      "description": "Local Ollama LLM provider for chat and prompts",
      "baseUrl": "http://localhost:11434"
    }
  },
  "defaults": {
    "server": "local-server",
    "provider": "ollama",
    "namespace": "jenkins",
    "docker_host": "npipe:////./pipe/docker_engine"
  }
}
